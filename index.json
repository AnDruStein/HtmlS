  import "index.js"
  local center "text-align: center"
  local advert "font-size: 21px; margin-top: 10px; margin-bottom: 5px"
  local button "background-color: #3D0C02; font-size: 118%; padding-top: 8px; border-radius: 18px; color: white; cursor: pointer;"
  local powered "font-size: 10px;"
  local top "margin-top: -25px"
  local olive "color: olive"
  local orange "color: darkorange"
    #*img @. { src="images/Darkbean(500x500).png" } [15]
